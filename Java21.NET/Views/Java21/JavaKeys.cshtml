@{
    ViewBag.Title = "注册码-21天学通Java";
    Layout = "~/Views/Shared/_LayoutJava.cshtml";
}@section head{
    <link href="/css/article.css" rel="stylesheet" />}
@using Vbes.WebControls.Mvc;
<div class="container">
    <div class="art">
        <table class="table text-center art-break">
            <tr>
                <td>状态</td>
                <td>注册码</td>
                <td>版本类型</td>
                <td>剩余时间</td>
                <td>剩余次数</td>
                <td>批次</td>
            </tr>
        @foreach (var info in Model)
        {
            <tr>
                <td>@if (info.valid)
                {
                    @Html.Raw("<img src='/images/ui_lgreen.png'/>");
                }else
                {
                    @Html.Raw("<img src='/images/ui_lred.png'/>");
                }</td>
                <td>@info.keys</td>
                <td>@info.ver</td>
                <td>@if (info.rdate > 0) {
                    @Html.Raw(info.rdate + "天")
                } else{
                    @Html.Raw("已过期")
                }</td>
                <td>@Html.Raw(info.residue + "次")</td>
                <td>@info.mark</td>
            </tr>
        }</table>
        <div class="pager">
            @Html.Pager((IPagedList)Model, new PagerOptions
        {
            PageIndexParameterName = "p",
            ShowPageIndexBox = false,
            FirstPageText = "首页",
            PrevPageText = "上一页",
            NextPageText = "下一页",
            LastPageText = "末页",
            ContainerTagName = "li"
        })</div>
    </div>
</div>